{
    "name": "vscode-ebnf",
    "displayName": "EBNF Tools",
    "description": "EBNF Tools for Visual Studio Code",
    "version": "1.2.0",
    "publisher": "igochkov",
    "author": {
        "name": "Ivaylo Gochkov",
        "url": "https://github.com/igochkov"
    },
    "icon": "images/icon.png",
    "engines": {
        "vscode": "^1.75.1"
    },
    "license": "MIT",
    "categories": [
        "Programming Languages",
        "Education"
    ],
    "keywords": [
        "ebnf",
        "bnf"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/igochkov/vscode-ebnf.git"
    },
    "homepage": "https://github.com/igochkov/vscode-ebnf",
    "bugs": {
        "url": "https://github.com/igochkov/vscode-ebnf/issues"
    },
    "sponsor": {
        "url": "https://github.com/sponsors/igochkov"
    },
    "contributes": {
        "languages": [
            {
                "id": "ebnf",
                "aliases": [
                    "EBNF",
                    "Extended Backus-Naur Form"
                ],
                "extensions": [
                    ".ebnf"
                ],
                "configuration": "syntaxes/ebnf.configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "ebnf",
                "scopeName": "source.ebnf",
                "path": "syntaxes/ebnf.tmLanguage.json"
            },
            {
                "scopeName": "markdown.ebnf.codeblock",
                "path": "./syntaxes/codeblock.json",
                "injectTo": [
                    "text.html.markdown"
                ],
                "embeddedLanguages": {
                    "meta.embedded.block.ebnf": "ebnf"
                }
            }
        ]
    },
    "dependencies": {
        "@types/vscode": "^1.79.1",
        "antlr4ts": "^0.5.0-alpha.4",
        "assert": "^2.0.0",
        "util": "^0.12.5"
    },
    "main": "./dist/extension.js",
    "scripts": {
        "vscode:prepublish": "npm run package",
        "webpack": "webpack --mode development",
        "package": "webpack --mode production --devtool hidden-nosources-source-map"
    },
    "devDependencies": {
        "ts-loader": "^9.4.3",
        "webpack": "^5.87.0",
        "webpack-cli": "^5.1.4"
    }
}
